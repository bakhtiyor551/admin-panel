{
  "name": "admin-panel-backend",
  "version": "1.0.0",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "pm2:start": "npx pm2 start ecosystem.config.cjs",
    "pm2:start:prod": "npx pm2 start ecosystem.config.cjs --env production",
    "pm2:stop": "npx pm2 stop gps-tracker-backend",
    "pm2:restart": "npx pm2 restart gps-tracker-backend",
    "pm2:reload": "npx pm2 reload gps-tracker-backend",
    "pm2:delete": "npx pm2 delete gps-tracker-backend",
    "pm2:logs": "npx pm2 logs gps-tracker-backend",
    "pm2:monit": "npx pm2 monit",
    "pm2:status": "npx pm2 status",
    "pm2:save": "npx pm2 save",
    "pm2:startup": "npx pm2 startup"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "mysql2": "^3.14.2",
    "pm2": "^5.3.0",
    "socket.io": "^4.7.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
